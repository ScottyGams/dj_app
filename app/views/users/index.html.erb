<h1>DJ's</h1>

<% @users.each_slice(4) do |users| %>
  <div class="row user-row">
    <% users.each do |user| %>
      <div class="span3">
        <%= image_tag user.image.thumb.url %>
        <h3><%= link_to user.name, user %></h3>
        <p><%= user.location %></p>
      </div>
    <% end %>
  </div>
<% end %>
